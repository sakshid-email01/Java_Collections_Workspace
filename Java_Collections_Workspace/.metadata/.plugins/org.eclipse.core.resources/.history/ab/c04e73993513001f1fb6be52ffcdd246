package com.example.demo;

import java.util.LinkedList;

public class SortAndReverse {

	public static void main(String[] args) {

		LinkedList<Integer> numbers1 = new LinkedList<Integer>();
		numbers1.add(4);
		numbers1.add(2);
		numbers1.add(1);
		numbers1.add(3);
	    for (int i : numbers1) {
	      System.out.println(i);
	    }
	   

	}

}
